<odoo>
    <data noupdate="1">
        <record id="ir_sequence_so_sample_sequence" model="ir.sequence">
            <field name="name">S.O Sample Sequence</field>
            <field name="code">s.o.sample.sequence</field>
            <field name="number_next_actual" eval="1" />
            <field name="prefix">SSO</field>
            <field name="padding">5</field>
        </record>

        <record id="ir_sequence_so_regular_sequence" model="ir.sequence">
            <field name="name">S.O Regular Sequence</field>
            <field name="code">s.o.regular.sequence</field>
            <field name="number_next_actual" eval="1" />
            <field name="prefix">RSO</field>
            <field name="padding">5</field>
        </record>

        <record id="ir_sequence_so_installation_sequence" model="ir.sequence">
            <field name="name">S.O Installation Sequence</field>
            <field name="code">s.o.installation.sequence</field>
            <field name="number_next_actual" eval="1" />
            <field name="prefix">ISO</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>